        "knplabs/knp-paginator-bundle": "~2.4.0",
        "friendsofsymfony/user-bundle": "2.0.*@dev"
    },
    "autoload": {
        "psr-0": { "Btn\\AdminBundle": "" }
    },
    "target-dir": "Btn/AdminBundle",
    "extra": {
