<!-- stylesheets start -->
    {# load btn base admin assets #}
    {% btn_asset 'btn_admin_base_css' %}
        <link rel="stylesheet" href="{{ asset_url }}" type="text/css">
    {% endbtn_asset %}
    {# load assets from asset storage #}
    {% for asset in btn_get_assets('stylesheets') %}
        {% btn_asset asset %}
            <link rel="stylesheet" href="{{ asset_url }}" type="text/css">
        {% endbtn_asset %}
    {% endfor %}
<!-- stylesheets end -->
