<!-- javascripts start -->
    {% if btn_bundle_is_enabled('BazingaJsTranslationBundle') %}
        {% btn_asset 'btn_admin_translator_js' %}
            <script src="{{ asset_url }}"></script>
        {% endbtn_asset %}
        <script src="{{ url('bazinga_jstranslation_js', { domain: 'btn_javascript' }) }}"></script>
    {% endif %}
    {# load btn base admin assets #}
    {% btn_asset 'btn_admin_base_js' %}
        <script src="{{ asset_url }}"></script>
    {% endbtn_asset %}
    {# load assets from asset storage #}
    {% for asset in btn_get_assets('javascripts') %}
        {% btn_asset asset %}
            <script src="{{ asset_url }}"></script>
        {% endbtn_asset %}
    {% endfor %}
    <script>
        // register ready callback
        jQuery(function() {
            BtnApp.ready();
        })
        // init app
        BtnApp.init();
    </script>
<!-- javascripts end -->
